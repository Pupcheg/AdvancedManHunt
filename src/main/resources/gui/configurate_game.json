{
  "$schema": "../../../../schema/gui.json",
  "key": "advancedmanhunt:configurate_game",
  "rows": 3,
  "background": "advancedmanhunt/configurate_game/background.png",
  "button_configurer": {
    "click_actions": [
      {
        "consumer": {
          "type": "sound",
          "sound": "minecraft:ui.button.click"
        }
      }
    ]
  },
  "buttons": [
    {
      "slot": 10,
      "texture": "advancedmanhunt/configurate_game/max_hunters.png",
      "name": "<color:yellow><lang:advancedmanhunt.gui.configurate_game.max_hunters.name>",
      "click_actions": [
        {
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.ConfigurateGameGui#handleModifyMaxHunters(ButtonClickContext)"
          }
        }
      ],
      "tickers": [
        {
          "at": "tick_end",
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.ConfigurateGameGui#tickMaxHunters(ButtonTickContext)"
          }
        }
      ]
    },
    {
      "slot": 11,
      "texture": "advancedmanhunt/configurate_game/max_spectators.png",
      "name": "<color:yellow><lang:advancedmanhunt.gui.configurate_game.max_spectators.name>",
      "click_actions": [
        {
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.ConfigurateGameGui#handleModifyMaxSpectators(ButtonClickContext)"
          }
        }
      ],
      "tickers": [
        {
          "at": "tick_end",
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.ConfigurateGameGui#tickMaxSpectators(ButtonTickContext)"
          }
        }
      ]
    },
    {
      "slot": 24,
      "texture": "advancedmanhunt/configurate_game/back.png",
      "name": "<color:red><lang:advancedmanhunt.gui.configurate_game.back.name>",
      "click_actions": [
        {
          "consumer": {
            "type": "open",
            "key": "advancedmanhunt:games_list"
          }
        }
      ]
    },
    {
      "slot": 25,
      "texture": "advancedmanhunt/configurate_game/discard.png",
      "name": "<color:red><lang:advancedmanhunt.gui.configurate_game.discard.name>",
      "click_actions": [
        {
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.ConfigurateGameGui#handleDiscard(ButtonClickContext)"
          }
        }
      ]
    },
    {
      "slot": 26,
      "texture": "advancedmanhunt/configurate_game/save.png",
      "name": "<color:green><lang:advancedmanhunt.gui.configurate_game.save.name>",
      "click_actions": [
        {
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.ConfigurateGameGui#handleSave(ButtonClickContext)"
          }
        }
      ]
    }
  ]
}