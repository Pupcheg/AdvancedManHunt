{
  "$schema": "../../../../schema/gui.json",
  "key": "advancedmanhunt:games_list",
  "rows": 6,
  "background": "advancedmanhunt/games_list/background.png",
  "button_configurer": {
    "click_actions": [
      {
        "consumer": {
          "type": "sound",
          "sound": "minecraft:ui.button.click"
        }
      }
    ]
  },
  "tickers": [
    {
      "at": "tick_end",
      "consumer": {
        "type": "reflect",
        "method": "me.supcheg.advancedmanhunt.gui.GamesListGui#acceptGuiTickEnd(GuiTickContext)"
      }
    }
  ],
  "buttons": [
    {
      "slot": 10,
      "texture": "advancedmanhunt/games_list/create.png",
      "name": "<color:#FF008C><lang:advancedmanhunt.gui.games_list.create.name>",
      "lore": [
        "<color:gray><lang:advancedmanhunt.gui.games_list.create.lore>"
      ],
      "click_actions": [
        {
          "consumer": {
            "type": "perform_command",
            "label": "advancedmanhunt game create"
          }
        }
      ]
    },
    {
      "slots": "#range 6-8,15-17,24-26,33-35,42-45,50-53",
      "default_shown": false,
      "texture": "advancedmanhunt/games_list/game.png",
      "name": "<red><lang:advancedmanhunt.gui.games_list.game.name>",
      "click_actions": [
        {
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.GamesListGui#acceptGameButtonClick(ButtonClickContext)"
          }
        }
      ],
      "tickers": [
        {
          "at": "tick_end",
          "consumer": {
            "type": "reflect",
            "method": "me.supcheg.advancedmanhunt.gui.GamesListGui#acceptGameButtonTickEnd(ButtonTickContext)"
          }
        }
      ]
    }
  ]
}